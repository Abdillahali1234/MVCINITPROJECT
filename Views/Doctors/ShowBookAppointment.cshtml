<h1>@Model.Name</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Time</th>
            <th scope="col">Name</th>
            <th scope="col">Date</th>
        </tr>
    </thead>
    <tbody>

        @{
            foreach (var item in Model.books)
            {
                <tr>
                    <th scope="row">@(item.AppointmentTime)</th>
                    <td>@item.Name</td>
                    <td>@item.AppointmentDate</td>
                </tr>
            }
        }

    </tbody>
</table>